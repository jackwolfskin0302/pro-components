    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@testing-library/react": "^10.0.2",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/classnames": "^2.2.7",
    "@types/enzyme": "^3.10.3",
    "@types/history": "^4.7.3",
    "@types/jest": "^26.0.0",
    "@types/lodash.isequal": "^4.5.5",
    "@types/memoize-one": "^5.1.2",
    "@types/node": "^14.0.26",
    "@types/qs": "^6.9.0",
    "@types/react": "^16.9.17",
    "@types/react-copy-to-clipboard": "^4.3.0",
    "@types/react-dom": "^16.9.1",
    "@types/react-helmet": "^6.0.0",
    "@types/react-intl": "^3.0.0",
    "@types/react-responsive": "^8.0.1",
    "@types/react-router-dom": "^5.1.0",
    "@types/warning": "^3.0.0",
    "@umijs/fabric": "^2.0.0",
    "@umijs/test": "^3.2.10",
    "@umijs/test-utils": "^3.2.10",
    "@umijs/utils": "^3.2.10",
    "antd": "^4.4.3",
    "babel-loader": "^8.1.0",
    "babel-plugin-import": "^1.12.2",
    "body-parser": "^1.18.2",
    "chalk": "^4.1.0",
    "cross-env": "^7.0.0",
    "dumi": "^1.0.13",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^7.2.0",
    "express": "^4.15.3",
    "father-build": "^1.18.1",
    "gh-pages": "^3.1.0",
    "husky": "^4.0.0",
    "jsdom": "^16.2.0",
    "jsdom-global": "^3.0.2",
    "lerna": "^3.20.2",
    "less-loader": "^6.0.0",
    "lint-staged": "^10.0.2",
    "mini-css-extract-plugin": "^0.9.0",
    "np": "^6.1.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "pify": "^5.0.0",
    "prettier": "^2.0.1",
    "query-string": "^6.11.1",
    "react": "^16.11.0",
